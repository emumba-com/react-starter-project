  "scripts": {
    "start": "webpack --progress --config webpack.server.config.js; UNIVERSAL_RENDERING=true node server/dist/build.js",
    "fast": "UNIVERSAL_RENDERING=false babel-node server/index.js",
    "build": "webpack --progress --config webpack.server.config.js",
    "jest": "NODE_ENV=development jest --no-cache",
    "flow": "flow"
  },
  "babel": {
    "presets": [
      "es2015-node5"
    ]
  },
  "jest": {
    "verbose": false,
