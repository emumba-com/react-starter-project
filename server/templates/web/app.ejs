    <link rel="stylesheet" type="text/css" href="/public_website/css/multiselect.css">
    <link rel="stylesheet" type="text/css" href="/<%- manifest['app.css'] %>">
    
    <style media="screen">
      html {
        font-family: 'Roboto', sans-serif;
        font-size: 62.5%;
      }

      body {
        line-height: 1.5;
        padding: 0;
        margin: 0;
      }
      
      h1 {
        font-weight: 300;
      }
    </style>
  </head>
  <body>
    <div id="root">
      <%- html %>
    </div>
    <script type="text/javascript">
      window.GAW = {
        preloadedState: <%- JSON.stringify(preloadedState) %>
      };
    </script>
